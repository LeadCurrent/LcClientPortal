@using Data
@using Web
@model SourcesViewModel
@{
    if (Model.MobileApp)
        Layout = "~/Pages/Shared/NoNavLayout.cshtml";
    else
        Layout = "~/Pages/Shared/TemplateLayout.cshtml";
}
@using (Html.BeginForm("SourcesList", "Sources", FormMethod.Post, new { id = "form", enctype = "multipart/form-data" }))
{
    <div id="partialview">
        @{
            await Html.RenderPartialAsync("PartialViews/SourcesList_Partial.cshtml");
        }
    </div>
}
<script>
    highlightcurrentpage('nav_Sources');
</script>