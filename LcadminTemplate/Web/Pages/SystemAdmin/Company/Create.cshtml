@model Data.CompanyViewModel
@using Web
@using Data

@{
    if (Model.MobileApp)
        Layout = "~/Pages/Shared/NoNavLayout.cshtml";
    else
    Layout = "~/Pages/Shared/TemplateLayout.cshtml";
}

@using (Html.BeginForm("Create", "Company", FormMethod.Post, new { id = "form", enctype = "multipart/form-data" }))
{
    <div id="partialview">
        @{
            await Html.RenderPartialAsync("PartialViews/Create_Partial.cshtml");
        }
    </div>
}
<script>
    highlightcurrentpage('nav_Companies');
</script>

<script>
    ValidateForm();
</script>