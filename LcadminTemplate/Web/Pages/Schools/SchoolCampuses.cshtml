@using Data
@using Web
@model Web.SchoolsViewModel
@{
    if (Model.MobileApp)
        Layout = "~/Pages/Shared/NoNavLayout.cshtml";
    else
        Layout = "~/Pages/Shared/TemplateLayout.cshtml";
}
@using (Html.BeginForm("CampusesBySchool", "Schools", FormMethod.Post, new { id = "form", enctype = "multipart/form-data" }))
{
    <div id="partialview">
        @{
            await Html.RenderPartialAsync("PartialViews/SchoolCampuses_Partial.cshtml");
        }
    </div>
}
<script>
    highlightcurrentpage('nav_Schools');
</script>